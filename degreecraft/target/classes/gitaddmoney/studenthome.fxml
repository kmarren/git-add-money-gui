<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="controllers.StudentHomepageController"
          alignment="center"
          hgap="10"
          vgap="10">

    <padding>
        <Insets top="25" right="25" bottom="25" left="25"/>
    </padding>

    <Label text="Welcome to the DegreeCraft !" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <Button text="Logout" onAction="#handleLogoutButtonAction" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
</GridPane>
